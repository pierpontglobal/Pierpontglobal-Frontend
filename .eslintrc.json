{
  "extends": [
    "plugin:flowtype/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "flowtype",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  }
}
